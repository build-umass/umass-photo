{
  "name": "umass-photo",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "lint": "eslint . && prettier --check ./**/*.ts ./**/*.tsx",
    "build": "next build",
    "start": "next start",
    "genmigration:supabase": "supabase db diff -f auto",
    "gentypes:supabase": "supabase gen types typescript --local > src/app/utils/supabase/database.types.ts",
    "start:supabase": "supabase start",
    "stop:supabase": "supabase stop --project-id=UMassPhoto --no-backup ; supabase stop --all",
    "restart:supabase": "npm run stop:supabase && npm run genmigration:supabase && npm run start:supabase && npm run gentypes:supabase",
    "dev": "next dev --turbopack",
    "dev:clean": "npm run stop:supabase && npm run start:supabase && next dev --turbopack",
    "test:vitest": "vitest run",
    "test:playwright": "playwright test",
    "test": "npm run test:vitest && npm run test:playwright",
    "test:clean": "npm run restart:supabase && npm run test",
    "test:ci": "npm run stop:supabase && act --use-gitignore=false --env-file none --artifact-server-path .artifacts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.87.1",
    "dotenv": "^16.4.7",
    "form-data": "^4.0.5",
    "mailgun.js": "^12.6.1",
    "next": "^15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.3.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/eslint": "^9.6.1",
    "@types/form-data": "^2.2.1",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/prettier": "^2.7.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/zxcvbn": "^4.4.5",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.2.0",
    "jsdom": "^27.3.0",
    "pg": "^8.16.3",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "supabase": "^2.45.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  }
}
